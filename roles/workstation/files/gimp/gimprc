# GIMP gimprc
# 
# This is your personal gimprc file.  Any variable defined in this file takes
# precedence over the value defined in the system-wide gimprc:
# /etc/gimp/2.0/gimprc
# Most values can be set within GIMP by changing some options in the
# Preferences dialog.

(brush-path "/mnt/Data/gimp/brushes:${gimp_dir}/brushes:${gimp_data_dir}/brushes")
(brush-path-writable "/mnt/Data/gimp/brushes:${gimp_data_dir}/brushes")
(import-raw-plug-in "${gimp_plug_in_dir}/plug-ins/file-raw-placeholder/file-raw-placeholder")
(snap-distance 4)
(monitor-xresolution 157)
(monitor-yresolution 161)
(icon-theme "Color")
(icon-size large)
(fill-options
    (style solid)
    (antialias yes)
    (feather no)
    (feather-radius 10))
(stroke-options
    (style solid)
    (antialias yes)
    (feather no)
    (feather-radius 10)
    (method line)
    (width 6)
    (unit pixels)
    (cap-style butt)
    (join-style miter)
    (miter-limit 10)
    (dash-offset 0)
    (dash-info 0)
    (emulate-brush-dynamics no))
(script-fu-path "/mnt/Data/gimp/scripts:${gimp_dir}/scripts:${gimp_data_dir}/scripts")


# end of gimprc
